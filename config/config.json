{
	"logs": {
		"MaxSize": 10,
		"MaxBackups": 5,
		"MaxAge": 30,
		"Compress": false
	},
	"tasks": [
		{
			"Collector": {
				"Mode": "syslog",
				"Addr": "127.0.0.1:8000"
			},
			"Parser": {
				"Mode": "jsonify"
			},
			"Handlers": [
				{
					"Mode": "stream",
					"Template": "${timestamp}|${url}|${name}"
				}
			]
		}
	]
}